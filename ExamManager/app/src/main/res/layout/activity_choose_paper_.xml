<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_normal"
    android:orientation="vertical"
    tools:context="com.example.spring.exammanager.core.activity.Choose_paper_Activity">

    <com.example.spring.exammanager.ui.widget.CommonTitile
        android:id="@+id/title_choose_paper"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></com.example.spring.exammanager.ui.widget.CommonTitile>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ListView
            android:id="@+id/lv_choose_paper"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:divider="@color/divider_color"
            android:dividerHeight="0.5dp" />

        <TextView
            android:id="@android:id/empty"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="200dp"
            android:text="暂无试卷"
            android:textColor="@color/font_value"
            android:textSize="15sp" />
    </FrameLayout>

</LinearLayout>
