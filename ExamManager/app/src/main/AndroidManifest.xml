<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.spring.exammanager">

    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".core.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".core.activity.RegisterActivity" />
        <activity android:name=".core.activity.AddPaperActivity" />
        <activity android:name=".core.activity.AddQuestionsActivity" />
        <activity android:name=".core.activity.MainOfStudentActivity" />
        <activity android:name=".core.activity.MainOfTeacherActivity" />
        <activity android:name=".core.activity.TestPaperManagerActivity" />
        <activity android:name=".core.activity.ChoosePaperActivity" />
        <activity android:name=".core.activity.StartTheTestActivity" />
        <activity android:name=".core.activity.ScoreSearchActivity" />
        <activity android:name=".core.activity.TestActivity" />
        <activity android:name=".core.activity.TestAct"></activity>
    </application>

</manifest>